{
  "name": "random-input-generator",
  "version": "1.0.0",
  "description": "Random data generator for testing.",
  "main": "index.js",
  "repository": "https://github.com/danielnovograd/random-input-generator",
  "scripts": {
    "test": "./node_modules/.bin/mocha --reporter spec",
    "compile": "babel --presets es2015 -d lib/ ./index.js",
    "testing": "nyc mocha"
  },
  "author": "Dan Novograd",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-preset-es2015": "^6.22.0",
    "chai": "*",
    "eslint": "^3.11.1",
    "eslint-plugin-react": "^6.7.1",
    "mocha": "*",
    "nyc": "^10.1.2"
  },
  "babel": {
    "presets": ["es2015"],
    "env": {
      "test": {
        "plugins": ["istanbul"]
      }
    }
  },
  "nyc": {
    "sourceMap": true,
    "instrument": false
  }
}
